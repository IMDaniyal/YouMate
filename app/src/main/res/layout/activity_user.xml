<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserInfo"
    android:orientation="vertical"
    android:background="@color/chip_color_light">

    <Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:title="User Information"
        android:background="@color/black"
        android:titleTextColor="@color/colorWhite"
        tools:ignore="NewApi" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou1"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvname"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Name"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="textEmailAddress"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou2"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvage"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Age"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="textEmailAddress"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou3"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvphone"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Phone"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="textEmailAddress"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou11"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvadd"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Address"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="textEmailAddress"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou4"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvbankname"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Bank Name"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="textEmailAddress"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou7"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvaccno"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="AccountNo"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="text"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou8"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvaccholdname"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Account Holder Name"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="text"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou9"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tveswaid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="eswaId"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="text"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou5"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvlevel"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Level"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="text"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textInputLayou6"
            android:layout_width="350dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/edittxt_circle_bg"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            >

            <TextView
                android:id="@+id/tvscoor"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bt_background"
                android:hint="Points"
                android:layout_marginTop="11dp"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:inputType="text"
                android:textColor="#639e42"
                android:textColorLink="#639e42" />
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nav1"
        android:layout_marginTop="230dp"
        android:layout_gravity="bottom"
        app:menu="@menu/botto_navigation"
        >

    </com.google.android.material.bottomnavigation.BottomNavigationView>
</LinearLayout>